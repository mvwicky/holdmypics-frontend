{
  "files.exclude": {
    "**/node_modules/": true,
    "_output/": true,
    "build": true
  },
  "css.validate": false,
  "tailwindCSS.validate": true,
  "eslint.codeActionsOnSave.mode": "all",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[svelte]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": false
    }
  },
  "prettier.disableLanguages": ["svelte"],
  "eslint.trace.server": "messages",
  "eslint.debug": true,
  "svelte.plugin.svelte.format.enable": false
}
